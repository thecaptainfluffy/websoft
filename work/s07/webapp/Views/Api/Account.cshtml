@{
    ViewData["Title"] = "Transfer between accounts";
    var account = (webapp.Models.Account)ViewData["Account"];
}
<h1>@ViewData["Title"]</h1>

<h4>@ViewData["Error"]</h2>
<form asp-controller="Api" asp-action="Post" method="post">
    From acc id: <input type="text" id="fNumber" name="fNumber">
    To acc id: <input type="text" id="tNumber" name="tNumber">
    How much: <input type="text" id="money" name="money">
    <input type="submit" value="Submit">
</form>
  
